<h1>Genre Name:</h1>
<h2><%= @genre.name %></h2>

<h1>Genre Songs:</h1>
<ul>
  <% @genre.songs.each do |song| %>
    <h2><li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li></h2>
  <% end %>
</ul>

<h1>Genre Artists:</h1>
<ul>
  <% @genre.artists.each do |artist| %>
    <h2><li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li></h2>
  <% end %>
</ul>
