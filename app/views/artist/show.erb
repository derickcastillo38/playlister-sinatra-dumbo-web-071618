<h1>Artist Name:</h1>
<h2><%= @artist.name %></h2>

<h1>Artist Songs:</h1>
<ul>
  <% @artist.songs.each do |song| %>
    <h2><li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li></h2>
  <% end %>
</ul>

<h1>Artist Genres:</h1>
<ul>
  <% @artist.genres.each do |genre| %>
    <h2><li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li></h2>
  <% end %>
</ul>
